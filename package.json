  "scripts": {
    "ng": "ng",
    "build-client": "ng build --configuration=production",
    "build-client-local": "ng build --configuration=local",
    "test-client": "ng test --code-coverage",
    "lint-client": "tslint --config tslint.json src/client/**/**.ts --exclude **/**.spec.ts --project src/client/tsconfig.json",
    "e2e": "ng e2e",
    "start": "node dist/server/index",
    "ssl": "node dist/server/ssl",
    "build-server": "tsc --project src/server --outDir dist/server --locale ja-jp",
    "lint-server": "tslint --config tslint.json src/server/**/**.ts --exclude **/**.spec.ts --project src/server/tsconfig.json",
    "lint": "npm run lint-client && npm run lint-server",
    "test": "npm run lint",
    "preversion": "npm run build2",
    "version": "git add -A",
    "clean": "rimraf src/**/*.spec.ts",
    "analyze": "webpack-bundle-analyzer dist/client/stats.json",
    "build2": "npm run build-server && npm run build-client",
    "deploy-development": "gcloud app deploy ./app.development.gcp.yaml --project smart-theater-development-sign",
    "deploy-test": "gcloud app deploy ./app.test.gcp.yaml --project smart-theater-test-sign",
    "deploy-production": "gcloud app deploy ./app.production.gcp.yaml --project smart-theater-production-sign --no-promote --version staging"
  },
  "private": true,
